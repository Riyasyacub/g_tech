<div id="<%= dom_id student %>">
  <p>
    <strong>Name:</strong>
    <%= student.name %>
  </p>

  <p>
    <strong>Roll no:</strong>
    <%= student.roll_no %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= student.address %>
  </p>

  <p>
    <strong>Courses:</strong>
    <%= student.courses %>
  </p>

  <p>
    <strong>Total fees:</strong>
    <%= student.total_fees %>
  </p>
</div>
